{
  "name": "shop",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "fastclick": "^1.0.6",
    "lodash": "^4.17.21",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "pinyin4js": "^1.3.18",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vant": "^2.4.2",
    "vue-cli-plugin-pxtorem": "^1.3.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "unitToConvert": "px",
        "viewportWidth": 375,
        "unitPrecision": 5,
        "propList": [
          "*"
        ],
        "viewportUnit": "vw",
        "fontViewportUnit": "vw",
        "minPixelValue": 1,
        "mediaQuery": false,
        "replace": false,
        "exclude": [],
        "landscape": false,
        "landscapeUnit": "vw",
        "landscapeWidth": "667"
      },
      "postcss-plugin-px2rem": {
        "rootValue": 37.5
      }
    }
  }
}
